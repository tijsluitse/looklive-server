<div class="feed-items">
    {{#each items}}
        <div class="feed-item" data-uuid="{{id}}">
            <img src="{{ image.url }}">
            <img class="avatar" src="{{ subject.avatar.url }}"><h2> {{ subject.name }}</h2>
            <h1><a href="appearance/{{id}}">{{title}}</a></h1>
        </div>
    {{/each}}
</div>