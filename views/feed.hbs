<section class="feedItems">
    {{#each items}}
        <article class="feedItem" data-uuid="{{id}}">
            <img src="{{ image.url }}">
            <img class="avatar" src="{{ subject.avatar.url }}">
            <h2> {{ subject.name }}</h2>
            <h1><a href="appearance/{{id}}">{{title}}</a></h1>
        </article>
    {{/each}}
</section>